{
    "name": "andresj-sanchez/as-miusage-api-plugin",
    "version": "1.0.0",
    "description": "Retrieves data from a remote API and displays it.",
    "type": "wordpress-plugin",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "Andres Sanchez",
            "email": "55501523+andresj-sanchez@users.noreply.github.com"
        }
    ],
    "require": {
        "php": ">=7.4"
    },
    "autoload": {
        "psr-4": {
            "AndresjSanchez\\AsMiusageApiPlugin\\": "src/"
        }
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.6",
        "wp-coding-standards/wpcs": "^2.3",
        "dealerdirect/phpcodesniffer-composer-installer": "1.0"
    },
    "scripts": {
        "phpcs": "phpcs src/",
        "phpcbf": "phpcbf src/"
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
